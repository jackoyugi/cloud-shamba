{{#partial "styles"}}
    <link rel='stylesheet' href='/css/styles.css'>
{{/partial}}
{{#partial "content"}}
    <h1>Order feeds</h1>

    <p>Add your order here:</p>

    <form action="/feeds" method="post" class="jumbotron">
        <div class="form-group">
            <label for="name">Feed Type</label>
            <input id="name" name="name" type="text" class="form-control">
        </div>
        <div class="form-group">
            <label for="price">Price</label>
            <select mdb-select md-form colorful-select dropdown-secondary id='feeds' name='feeds' type='text'>
                <option class="text-grey" value="" disabled selected>Feed type</option>
                <option qunatity="50kgs" value="2000">Layers mash</option>
                <option qunatity="50kgs" value="1500">Kienyeji Mash</option>
            </select>
        </div>

        <div class="form-group">
            <label for="location">Location</label>
            <input value="" id="breed" name="breed" type="text" placeholder="enter location" required>
        </div>
        <h1>My First Google Map</h1>

        <div id="googleMap" style="width:100%;height:400px;"></div>

        <script>
            function myMap() {
                var mapProp= {
                center:new google.maps.LatLng(51.508742,-0.120850),
                 zoom:5,
                    };
            var map = new google.maps.Map(document.getElementById("googleMap"),mapProp);
            }
         </script>

        <script src="https://maps.googleapis.com/maps/api/js?key=YOUR_KEY&callback=myMap"></script>

        <button type="submit" class="btn btn-outline-success">Add Feeds</button>
    </form>
{{/partial}}
{{> layout.hbs}}